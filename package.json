{
  "name": "v-folder",
  "version": "1.3.0",
  "description": "vue tree select component",
  "main": "dist/build.js",
  "scripts": {
    "mock": "node ./server/index.js",
    "watchify": "watchify -vd -g envify src/demo.js -o dist/demo.js",
    "serve": "http-server -o -s -c 1 -a localhost",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel mock watchify serve",
    "build:min": "cross-env NODE_ENV=production browserify -g envify src/index.js -s VFolder | uglifyjs -c warnings=false -m > dist/build.min.js",
    "build": "cross-env NODE_ENV=production browserify -g envify src/index.js -s VFolder > dist/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AngusFu/v-folder.git"
  },
  "author": "wemlion <angusfu1126@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AngusFu/v-folder/issues"
  },
  "homepage": "https://github.com/AngusFu/v-folder#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "vue": "^2.1.10"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
